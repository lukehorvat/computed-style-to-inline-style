{
  "name": "computed-style-to-inline-style",
  "version": "2.0.0",
  "description": "Convert a HTML element's computed CSS to inline CSS.",
  "author": "Luke Horvat",
  "license": "MIT",
  "repository": "lukehorvat/computed-style-to-inline-style",
  "main": "dist/main.js",
  "module": "src/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build:test": "webpack ./test.js --o ./dist/test.js --mode=development",
    "build:dev": "webpack --mode=development",
    "pretest": "webpack ./test.js --o ./dist/test.js --mode=development",
    "test": "mocha dist/test.js",
    "build": "webpack --module-bind js=babel-loader --mode=production"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "babel-loader": "^8.0.0-beta.3",
    "chai": "^4.1.2",
    "electron": "1.7.9",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "mocha": "^5.2.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4"
  },
  "keywords": [
    "css",
    "style",
    "stylesheet",
    "computed",
    "inline",
    "element",
    "html",
    "browser",
    "dom"
